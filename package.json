{
  "name": "elm-serverless",
  "version": "3.0.2",
  "description": "Deploy an Elm worker to AWS Lambda using serverless",
  "main": "index.js",
  "scripts": {
    "clobber": "rm -rf node_modules elm-stuff demo/node_modules demo/elm-stuff tests/elm-stuff",
    "postinstall": "elm package install -y",
    "demo:install": "cd demo && npm install",
    "demo:deploy": "cd demo && npm run deploy",
    "start": "cd demo && npm start",
    "test": "elm test",
    "posttest": "eslint ."
  },
  "author": "Kevin Tonon <kevin@betweenconcepts.com>",
  "license": "MIT",
  "dependencies": {
    "guid": "0.0.12",
    "xmlhttprequest": "git://github.com/ktonon/node-XMLHttpRequest.git#bdd9e5c75a3c0f8f4038697234e8612270cec799"
  },
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-test": "^0.18.7",
    "eslint": "^3.13.1",
    "eslint-config-es2015": "^1.1.0"
  }
}
