{
  "name": "elm-serverless-demo",
  "version": "1.0.0",
  "description": "Small demonstrative AWS Lambda API written in elm",
  "main": "api.js",
  "scripts": {
    "deploy": "cross-env NODE_ENV=production serverless deploy",
    "lint": "eslint .",
    "postinstall": "elm package install -y && cd .. && npm install",
    "posttest": "npm run lint",
    "start": "concurrently --kill-others 'npm run server' 'npm run watch'",
    "server": "serverless offline",
    "watch": "webpack --watch"
  },
  "author": "kevin@betweenconcepts.com",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "cross-env": "^3.1.4",
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.1.1",
    "eslint": "^3.13.1",
    "eslint-config-es2015": "^1.1.0",
    "rc": "^1.1.6",
    "serverless": "^1.17.0",
    "serverless-offline": "^3.15.1",
    "serverless-webpack": "^2.0.0",
    "shebang-loader": "0.0.1",
    "strip-debug-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.3.0",
    "webpack-loader": "^0.0.1"
  }
}
